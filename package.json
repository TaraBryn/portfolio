{
  "name": "tarabryn",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build:css": "node-sass --include-path public -o public/css -r src/scss/",
    "build:project-css": "node-sass --include-path public/project-files -o public/project-files -r public/project-files",
    "webpack:dev": "webpack --mode development",
    "start:dev": "nodemon -x \"npm run start\"",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "watch:project-css": "nodemon -e scss -x \"npm run build:project-css\"",
    "webpack:production": "webpack --mode production"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^3.21.2",
    "highlight.js": "^9.16.2",
    "http-errors": "~1.6.3",
    "marked": "^0.7.0",
    "mongodb": "^3.3.5",
    "morgan": "~1.9.1",
    "node-gyp": "^6.0.1",
    "prop-types": "^15.7.2",
    "pug": "^2.0.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sanitize-html": "^1.20.1",
    "sanitize-html-react": "^1.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "html-loader": "^0.5.5",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
